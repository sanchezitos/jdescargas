<center><a href="/category/marvel"><img class="side" width="120px;" height="50px;" src="https://res.cloudinary.com/jdescargas/image/upload/v1591413884/img/MarvelClic_yk7vht.png"></a></center>
<div class="swiper-container">
	<div class="swiper-wrapper ">
		{{#each marvel}}
		<div class="swiper-slide" id="imagen" >		
			<a href="/movies/{{title}}">
			<div class="div-imagen" style="margin-bottom: 20px; margin-right: 10px;  ">
				<div class="divimg">
					<div class="cat">
						<center><span style="color:whitesmoke; padding: 10px; font-size:20; "><b>{{title}}</b></span>
						</center>
					</div>
					<FONT FACE="arial narrow">
						<p style="color:gray">{{description}}</p>
					</FONT>
					<div style="padding-top: 75px; color:gray;">
					</div>
				</div>
				{{#if filename}}
				<img class="desvanecer side" style="border-radius: 10px 10px 10px 10px;
-moz-border-radius: 10px 10px 10px 10px;
-webkit-border-radius: 10px 10px 10px 10px;
border: 0px solid #000000; height: 15.5rem; width: 15.5rem;
" src="/upload/{{filename}}" />
				{{else}}
				<img class="desvanecer side" style="border-radius: 10px 10px 10px 10px;
-moz-border-radius: 10px 10px 10px 10px;
-webkit-border-radius: 10px 10px 10px 10px;
border: 0px solid #000000; height: 15.5rem; width: 15.5rem;
" src="{{imageURL}}" />
				{{/if}}
			</div>
			</a>
		</div>
		{{/each}}
	</div>
	<!-- Add Pagination -->
	<div class="swiper-pagination"></div>
</div>
